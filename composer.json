{
  "name": "hphio/util",
  "description": "Various convenience and utility classes",
  "license": "proprietary",
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload-dev": {
    "psr-0": {
      "tests\\": "tests"
    },
    "psr-4": {
      "tests\\": "tests"
    }
  },
  "autoload": {
    "psr-0": {
