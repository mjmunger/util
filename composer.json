    }
  },
  "require": {
    "league/container": "^4.2",
    "hphio/queryrunner": "^1.0",
    "ext-simplexml": "*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@git.hph.io:hph/queryrunner.git"
    }
  ]
}
